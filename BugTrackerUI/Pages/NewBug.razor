@page "/new-bug"

<h3>Add New Bug</h3>

<EditForm Model="AddBug">
    <InputText @bind-value="@AddBug.Title"> Enter Title</InputText>
    <InputText @bind-value="@AddBug.Description"> Enter Description</InputText>
    <InputText @bind-value="@AddBug.Priority"> Enter Priority</InputText>
    <button type="submit">Add Bug</button>
</EditForm>

@code {
    public Bug AddBug { get; set; } = new Bug();
}
